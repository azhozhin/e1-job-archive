<?xml version="1.0" encoding="UTF-8"?>
<properties>
	<cssNamedQueries>
		<cssNamedQuery name="vacancyProperty">
			<cssQuery>td[width=30%%][align=right][valign=top] > strong:contains(%s)</cssQuery>
			<cssArgsCount>1</cssArgsCount>
		</cssNamedQuery>
	</cssNamedQueries>

	<jobSectionProperties>
		<entry>
			<key>href</key>
			<cssQuery>div[id~=secion_\\d] a[href~=section=\\d]</cssQuery>
			<cssQueryArgs/>
			<transformClass/>
		</entry>
	</jobSectionProperties>
	
	<jobListProperties>
		<entry>
			<key>href</key>
			<cssQuery>div[id~=vacancy_item_\\d] a[href~=id=\\d]</cssQuery>
			<cssQueryArgs/>
			<transformClass/>
		</entry>
	</jobListProperties>
	
	<vacancyProperties>
		<entry>
			<key>jobTitle</key>
			<cssQuery namedQuery="vacancyProperty"/>
			<cssQueryArgs>
				<arg>Должность</arg>
			</cssQueryArgs>
			<transformClass>ru.xrm.app.transformers.JobPropertyTransform</transformClass>
		</entry>  
		<entry>
			<key>dutyType</key>
			<cssQuery namedQuery="vacancyProperty"/>
			<cssQueryArgs>
				<arg>Занятость</arg>
			</cssQueryArgs>
			<transformClass>ru.xrm.app.transformers.JobPropertyTransform</transformClass>
		</entry>
		<entry>
			<key>education</key>
			<cssQuery namedQuery="vacancyProperty"/>
			<cssQueryArgs>
				<arg>Образование</arg>
			</cssQueryArgs>
			<transformClass>ru.xrm.app.transformers.JobPropertyTransform</transformClass>
		</entry> 
		<entry>
			<key>experience</key>
			<cssQuery namedQuery="vacancyProperty"/>
			<cssQueryArgs>
				<arg>Опыт работы</arg>
			</cssQueryArgs>
			<transformClass>ru.xrm.app.transformers.JobPropertyTransform</transformClass>
		</entry>	
		<entry>
			<key>schedule</key>
			<cssQuery namedQuery="vacancyProperty"/>
			<cssQueryArgs>
				<arg>График работы</arg>
			</cssQueryArgs>
			<transformClass>ru.xrm.app.transformers.JobPropertyTransform</transformClass>
		</entry> 
		<entry>
			<key>kindOfBusiness</key>
			<cssQuery namedQuery="vacancyProperty"/>
			<cssQueryArgs>
				<arg>Отрасль</arg>
			</cssQueryArgs>
			<transformClass>ru.xrm.app.transformers.JobPropertyTransform</transformClass>
		</entry> 
		<entry>
			<key>city</key>
			<cssQuery namedQuery="vacancyProperty"/>
			<cssQueryArgs>
				<arg>Город</arg>
			</cssQueryArgs>
			<transformClass>ru.xrm.app.transformers.JobPropertyTransform</transformClass>
		</entry>
		<entry>
			<key>employer</key>
			<cssQuery namedQuery="vacancyProperty"/>
			<cssQueryArgs>
				<arg>Работодатель</arg>
			</cssQueryArgs>
			<transformClass>ru.xrm.app.transformers.JobPropertyTransform</transformClass>
		</entry>
		<entry>
			<key>date</key>
			<cssQuery namedQuery="vacancyProperty"/>
			<cssQueryArgs>
				<arg>Дата</arg>
			</cssQueryArgs>
			<transformClass>ru.xrm.app.transformers.JobPropertyTransform</transformClass>
		</entry>
		<entry>
			<key>contactInformation</key>
			<cssQuery namedQuery="vacancyProperty"/>
			<cssQueryArgs>
				<arg>Контактная информация</arg>
			</cssQueryArgs>
			<transformClass>ru.xrm.app.transformers.JobPropertyTransform</transformClass>
		</entry> 
		<entry>
			<key></key>
			<cssQuery namedQuery="vacancyProperty"/>
			<cssQueryArgs>
				<arg>Вакансия предоставлена</arg>
			</cssQueryArgs>
			<transformClass>ru.xrm.app.transformers.JobPropertyTransform</transformClass>
		</entry>
		<entry>
			<key>body</key>
			<cssQuery>td[colspan=2][valign=top]</cssQuery>
			<cssQueryArgs/>
			<transformClass/>
		</entry>
	</vacancyProperties>
</properties>
