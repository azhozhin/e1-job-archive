<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">

	<!-- Config here. Can even be kept empty. -->
 
	<managed-bean>
		<managed-bean-name>visitor</managed-bean-name>
		<managed-bean-class>ru.xrm.app.webapp.model.Visitor</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>search</managed-bean-name>
		<managed-bean-class>ru.xrm.app.webapp.model.Search</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
 

</faces-config>